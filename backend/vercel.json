{
  "version": 2,
  "buildCommand": "npm install",
  "builds": [
    {
      "src": "node_modules/manifest/dist/manifest/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "node_modules/manifest/dist/manifest/src/main.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS,PATCH"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type,Authorization,X-App-ID,X-Requested-With"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    },
    {
      "source": "/health",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type,Authorization,X-App-ID,X-Requested-With"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "TOKEN_SECRET_KEY": "46727307bc538aff0f7c941f011f00142f63c785476784b3a52f10fe8640dc8c",
    "BASE_URL": "https://no-code-app-a051cc89-1f87-40f1-bb8d-8e33dfa11eab.vercel.app",
    "PORT": "1111",
    "OPEN_API_DOCS": "true",
    "CORS_ORIGIN": "*",
    "CORS_CREDENTIALS": "true",
    "CORS_METHODS": "GET,POST,PUT,DELETE,OPTIONS,PATCH",
    "CORS_HEADERS": "Content-Type,Authorization,X-App-ID,X-Requested-With"
  }
}